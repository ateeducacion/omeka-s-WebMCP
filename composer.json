{
    "name": "erseco/omeka-s-webmcp",
    "description": "WebMCP module for Omeka S - exposes management capabilities to AI agents",
    "type": "omeka-s-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Área de Tecnología Educativa",
            "email": "ate.educacion@gobiernodecanarias.org"
        }
    ],
    "homepage": "https://github.com/erseco/omeka-s-WebMCP",
    "support": {
        "issues": "https://github.com/erseco/omeka-s-WebMCP/issues"
    },
    "keywords": [
        "Omeka S",
        "module",
        "WebMCP",
        "AI",
        "MCP"
    ],
    "require": {
        "php": ">=8.1"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "omeka/composer-addon-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*",
        "zerocrates/extract-tagged-strings": "dev-master",
        "laminas/laminas-form": "^3.4",
        "laminas/laminas-mvc": "^3.8",
        "laminas/laminas-session": "^2.25"
    },
    "autoload": {
        "psr-4": {
            "WebMCP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebMCPTest\\": "test/WebMCPTest/"
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/zerocrates/extract-tagged-strings.git"
        }
    ]
}
